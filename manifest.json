{
  "manifest_version": 3,
  "name": "SafeSurf - Porn Blocker",
  "version": "1.0",
  "description": "A content filtering extension that blocks inappropriate content and redirects to a positive page",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["redirect.html", "style.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    // "16": "icon16.png",
    // "48": "icon48.png",
    // "128": "icon128.png"
  }
} 